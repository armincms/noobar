{
    "name": "armincms/noobar",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "armincms/armincms": "^0.3.3", 
        "armincms/blogger": "~0.3", 
        "armincms/chain": "~0.3",
        "armincms/sofre-api": "*",
        "armincms/sanctum-login": "*",
        "optimistdigital/nova-multiselect-field": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Noobar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [ 
                "Armincms\\Noobar\\NoobarServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
